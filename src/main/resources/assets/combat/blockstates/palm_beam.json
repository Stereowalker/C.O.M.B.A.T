{
    "multipart": [
        {   "apply": { "model": "combat:block/palm_beam_center" }},
        {   "when": { "north": "true" },
            "apply": { "model": "combat:block/palm_beam_side", "uvlock": true }
        },
        {   "when": { "east": "true" },
            "apply": { "model": "combat:block/palm_beam_side", "y": 90, "uvlock": true }
        },
        {   "when": { "south": "true" },
            "apply": { "model": "combat:block/palm_beam_side", "y": 180, "uvlock": true }
        },
        {   "when": { "west": "true" },
            "apply": { "model": "combat:block/palm_beam_side", "y": 270, "uvlock": true }
        },
        {   "when": { "up": "true" },
            "apply": { "model": "combat:block/palm_beam_side", "x": 270, "uvlock": true }
        },
        {   "when": { "down": "true" },
            "apply": { "model": "combat:block/palm_beam_side", "x": 90, "uvlock": true }
        }
    ]
}
